<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.1.0</Version>
    <Authors>Alexander Selishchev</Authors>
    <Company>Alexander Selishchev</Company>
    <Product>EmailValidator.NET</Product>
    <Description>C# SMTP and format email validator</Description>
    <PackageLicense>https://github.com/adoconnection/EmailValidator.NET/blob/master/LICENSE.md</PackageLicense>
    <PackageProjectUrl>https://github.com/adoconnection/EmailValidator.NET</PackageProjectUrl>
